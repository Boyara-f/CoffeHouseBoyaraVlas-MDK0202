        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoffeHouseBoyaraVlas"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock x:Name="IdShow"/>
            <Button x:Name="btn_changeProd" Content="Добавить продукт" Click="btn_changeProd_Click"/>
            <Button x:Name="btn_editAccount" Content="Акк" Click="btn_editAccount_Click"/>
        </StackPanel>
        
    </Grid>
</Window>
